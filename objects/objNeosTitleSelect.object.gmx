    if moveUp 
    {
        if selector &gt; 0
            selector--;
        else
            selector = 3;
            
            
        audio_play_sound(sndGT_UIFX1, 0, false);
    }
    
    if moveDown 
    {
        if selector &lt; 3
            selector++;
        else
            selector = 0;
            
            
        audio_play_sound(sndGT_UIFX1, 0, false);
    }
    
