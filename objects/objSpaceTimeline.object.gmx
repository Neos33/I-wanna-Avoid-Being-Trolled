            <string>///Timeline

t += 1;

switch (t)
{
    case 1: with objPlayer { frozen = true; } break;
    // Scene 1 (going up and slowly turning into space)
    case 40:
        alarm[0] = 1;
    break;
    
    case 100:
        instance_create(0,0,objSpaceCutsceneTileMove);
    break;

    case 230:
        with objNeosSpaceJump 
        {
            image_alpha = 0;
            sprite_index = sprPlayerJump_Dark;
            depth = -1;
        }
    break;
    case 432:
        background_visible[0] = false;
        background_visible[1] = false;
        background_visible[2] = false;
    break;
    
    case 566:
        with objNeosSpaceJump { fadeIn = false; }
    break;
    
    case 600:
        alarm[0] = 0; //stop the particles
        background_visible[4] = true;
    break;
    
    case 603:
        with objGMS2_Watermark { newColor = c_white; }
    break;
    
    case 650:
        instance_destroy(objNeosSpaceJump);
        instance_create(400,304,objNeosSpaceJump)
    break;
    
    case 800:
        with objNeosSpaceJump 
        {
            gravity = 0.2;
            gravity_direction = 90;
            vspeed = 4;
            moveToCenter = false;
        }
        
    break;
    
    case 950:
        background_vspeed[4] = 0;
    break;
    
    case 970:
        alarm[2] = 1; //Start Fade In screen
    break;
    
    // Scene 2 (earth look)
    case 1050:
        alarm[3] = 1; //Start Fade Out screen
        alarm[0] = 0;
        part_system_clear( pS );

        background_visible[3] = true; //Earth BG
        background_visible[4] = false; //Space
        with( objPlayerKiller )
        {
            if object_index != objSpikeDown
                visible = 1;
        }

        background_color = c_black;
        with objNeosSpaceJump { instance_destroy(); } //Change this later on
        event_user(0);
    break;
    
    case 1390:
        with objNeosSpaceJump { instance_destroy(); }
        instance_create(400,304,objNeosSpaceJump);
        background_visible[3] = false; //Earth BG
        background_visible[4] = true; //Space
        background_vspeed[4] = 18;
        with( objPlayerKiller )
        {
            if object_index = objSpikeDown
            {
                visible = 1;
                y-=352;
            }
            else
                visible = 0;
        }
    break;
    
    case 1796:
        audio_sound_gain(BGM,0,5000);
    break;
    
    case 1916: instance_create(0,0,objZoomIn); break;
    case 2034:
    case 2090:
        with objSpikeDown { y+=1; }
    break;
    
    case 2130:
        with objSpikeDown { y+=1; }
        audio_play_sound(sndTing,0,false);
    break;
    
    case 2150:
        objPlayer.x = objNeosSpaceJump.x;
        objPlayer.y = objNeosSpaceJump.y;
        scrKillPlayer();
        instance_destroy(objNeosSpaceJump);
    break;
    /*case 1440: 
    case 1450: 
    case 1460: 
    case 1470: 
    case 1480: 
    case 1490: 
    case 1500: 
        with objSpikeDown { y+=1; }
        if background_vspeed[4] &gt; 0 { background_vspeed[4] -= 1; }
    break;*/
}

